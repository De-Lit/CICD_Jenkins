#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent none
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        agent any
        stage("before_script") {
            sh 'until docker info; do sleep 1; done'
        }
        stage("build_cat") {
            agent {
                docker { image 'gcc' }
            }
            steps {
                echo "The test message build-cat"
                sh 'gcc -v'
            }
        }
        stage("Second step") {
            agent any
            steps {
                echo "Second step"
            }
        }
    }
}
