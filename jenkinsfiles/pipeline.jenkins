#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("build_cat") {
            agent {
                docker { image 'gcc' }
                reuseNode true
            }
            steps {
                echo "The test message build-cat"
                sh 'gcc --version'
            }
        }
        stage("Second step") {
            steps {
                echo "Second step"
            }
        }
    }
}
